<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>AdPunto Notificaciones v.01 - aaldev</name>
    <description>
        Aplicación de notificaciónes de mensajes de sistema AdPunto
    </description>
    <author email="alejandro@adp.mx" href="http://adpunto.mx">
        AdPunto Inbound Marketing
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="http://adpdev.com" />
    <access launch-external="yes" origin="http://t.adp.mx/*" />
    <access launch-external="yes" origin="http://adpdev.com/*" />
    <allow-intent href="http://t.adp.mx/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="ldpi" src="www/res/icon/android/mipmap-ldpi/icon.png" />
    <icon density="mdpi" src="www/res/icon/android/mipmap-mdpi/icon.png" />
    <icon density="hdpi" src="www/res/icon/android/mipmap-hdpi/icon.png" />
    <icon density="xhdpi" src="www/res/icon/android/mipmap-xhdpi/icon.png" />
    <splash density="land-ldpi" src="www/res/icon/android/drawable-land-ldpi/screen.png" />
    <splash density="land-mdpi" src="www/res/icon/android/drawable-land-mdpi/screen.png" />
    <splash density="land-hdpi" src="www/res/icon/android/drawable-land-hdpi/screen.png" />
    <splash density="land-xhdpi" src="www/res/icon/android/drawable-land-xhdpi/screen.png" />
    <splash density="port-ldpi" src="www/res/icon/android/drawable-port-ldpi/screen.png" />
    <splash density="port-mdpi" src="www/res/icon/android/drawable-port-mdpi/screen.png" />
    <splash density="port-hdpi" src="www/res/icon/android/drawable-port-hdpi/screen.png" />
    <splash density="port-xhdpi" src="www/res/icon/android/drawable-port-xhdpi/screen.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
</widget>
